  "scripts": {
    "test": "NODE_ENV=test karma start",
    "start": "npm install && node_modules/.bin/typings install && npm run-script dev",
    "dev": "node node_modules/.bin/webpack-dev-server --content-base app --host 0.0.0.0",
    "build": "npm test && rm -rf dist/ && NODE_ENV=production node node_modules/.bin/webpack --bail && cp app/index.html dist/index.html",
    "dist": "npm test && rm -rf dist/ && NODE_ENV=production node node_modules/.bin/webpack --bail && cp app/index.html dist/index.html && NODE_ENV=production  node node_modules/.bin/webpack-dev-server --content-base dist --host 0.0.0.0"
  },
