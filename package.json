{
  "name": "angular-webpack",
  "version": "1.0.0",
  "description": "Skeleton project using Angular 1.4.7, typescript and webpack",
  "main": "app/index.js",
  "repository": "https://github.com/deonclem/angular-webpack-typescript.git",
  "dependencies": {
    "angular": "^1.4.7",
    "angular-animate": "^1.4.7",
    "angular-aria": "^1.4.7",
    "angular-material": "^0.11.4",
    "ui-router": "^0.2.15"
  },
  "devDependencies": {
    "angular-mocks": "^1.4.7",
    "babel-loader": "^5.3.2",
    "chai": "^3.4.0",
    "css-loader": "^0.21.0",
    "jasmine-core": "^2.3.4",
    "karma": "^0.13.14",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs2-launcher": "^0.3.2",
    "karma-requirejs": "^0.2.2",
    "karma-spec-reporter": "0.0.22",
    "karma-typescript-preprocessor": "0.0.21",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.3",
    "ng-annotate-loader": "0.0.10",
    "raw-loader": "^0.5.1",
    "requirejs": "^2.1.21",
    "style-loader": "^0.13.0",
    "stylus-loader": "^1.4.0",
    "ts-loader": "^0.7.1",
    "tsd": "^0.6.5",
    "typescript": "^1.6.2",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1"
  },
  "scripts": {
    "test": "NODE_ENV=test karma start",
    "start": "npm install && node_modules/.bin/tsd install && npm run-script dev",
    "dev": "node node_modules/.bin/webpack-dev-server --content-base app",
    "build": "NODE_ENV=production node node_modules/.bin/webpack && cp app/index.html dist/index.html",
    "dist": "NODE_ENV=production node node_modules/.bin/webpack && cp app/index.html dist/index.html && node node_modules/.bin/webpack-dev-server --content-base dist"
  },
